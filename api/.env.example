# Google OAuth credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Session
SESSION_SECRET=your_session_secret_here

# Base URL and client URL
BASE_URL=http://localhost:3000
CLIENT_URL=http://localhost:3000

# Google OAuth callback URLs - these must exactly match what's in Google Cloud Console
CALLBACK_URL=http://localhost:3000/api/auth/google/callback
REDIRECT_ORIGIN=http://localhost:3000

# Supabase credentials
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Environment
NODE_ENV=development
PORT=3000

# Auth redirect
AUTH_REDIRECT_URL=http://localhost:3001/?auth_success=true
